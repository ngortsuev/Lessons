@model IEnumerable<CityApp.Domain.Entities.Continent>

@{
    ViewBag.Title = "Menu";
}

@Html.ActionLink("Home", "List", "Continent")

@foreach(var item in Model)
{
    @Html.RouteLink(item.Name,
        new {
            controller = "Country",
            action = "List",
            continent = item.Id
        }
    )
}

